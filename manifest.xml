<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>b2978fce-d998-4f29-865c-f34d5e2fc78d</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Contoso</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Orhan101124"/>
  <Description DefaultValue="A template to get started."/>
  <IconUrl DefaultValue="https://orhanelturk.github.io/Orhan2/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://orhanelturk.github.io/Orhan2/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
    <AppDomain>https://www.contoso.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://orhanelturk.github.io/Orhan2/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="CustomTab" label="Solar Add-In">
              <!-- Group 1: Project Info -->
              <Group id="Group1_ProjectInfo" label="Project Info">
                <Control xsi:type="Button" id="InputSheet" label="Input Sheet" icon="icon-input-sheet.png" action="ShowInputSheet" size="Small"/>
                <Control xsi:type="Button" id="Loads" label="Loads" icon="icon-loads.png" action="ShowLoads" size="Small"/>
                <Control xsi:type="Button" id="ProjectLocation" label="Project Location" icon="icon-location.png" action="ShowProjectLocation" size="Small"/>
                <Control xsi:type="Button" id="Tariffs" label="Tariffs" icon="icon-tariffs.png" action="ShowTariffs" size="Small"/>
              </Group>

              <!-- Group 2: Solar Resource Data -->
              <Group id="Group2_SolarResourceData" label="Solar Resource Data">
                <Control xsi:type="Button" id="ImportWeatherData" label="Import Hourly Weather Data" icon="icon-weather.png" action="ImportWeatherData" size="Small"/>
                <Control xsi:type="Button" id="GetPVGIS" label="Get From PVGIS" icon="icon-pvgis.png" action="GetFromPVGIS" size="Large"/>
              </Group>

              <!-- Group 3: Design -->
              <Group id="Group3_Design" label="Design">
                <Control xsi:type="Button" id="SolarLayout" label="Solar Layout" icon="icon-solar-layout.png" action="ShowSolarLayout"/>
                <Control xsi:type="Button" id="ConsumptionVsProduction" label="Consumption Vs Production" icon="icon-consumption-production.png" action="ShowConsumptionVsProduction"/>
                <Control xsi:type="Button" id="ElectricalDesign" label="Electrical Design" icon="icon-electrical-design.png" action="ShowElectricalDesign"/>
              </Group>

              <!-- Group 4: Simulation -->
              <Group id="Group4_Simulation" label="Simulation">
                <Control xsi:type="Button" id="Simulation" label="Simulation" icon="icon-simulation.png" action="RunSimulation"/>
              </Group>

              <!-- Group 5: Project Costs -->
              <Group id="Group5_ProjectCosts" label="Project Costs">
                <Control xsi:type="Button" id="CAPEX" label="CAPEX" icon="icon-capex.png" action="ShowCAPEX"/>
                <Control xsi:type="Button" id="OPEX" label="OPEX" icon="icon-opex.png" action="ShowOPEX"/>
              </Group>

              <!-- Group 6: Financial Model -->
              <Group id="Group6_FinancialModel" label="Financial Model">
                <Control xsi:type="Button" id="ModelInputs" label="Model Inputs" icon="icon-model-inputs.png" action="ShowModelInputs" size="Small"/>
                <Control xsi:type="Button" id="FinancialResults" label="Financial Results" icon="icon-financial-results.png" action="ShowFinancialResults"/>
              </Group>

              <!-- Group 7: Optimization -->
              <Group id="Group7_Optimization" label="Optimization">
                <Control xsi:type="Button" id="CompareLands" label="Compare Different Lands" icon="icon-lands.png" action="CompareLands"/>
                <Control xsi:type="Button" id="CompareComponents" label="Compare Different Components" icon="icon-components.png" action="CompareComponents"/>
                <Control xsi:type="Button" id="CompareCountries" label="Compare Countries" icon="icon-countries.png" action="CompareCountries"/>
                <Control xsi:type="Button" id="OptimizeSolution" label="Optimize Solution" icon="icon-optimize.png" action="OptimizeSolution"/>
              </Group>

              <!-- Group 8: Outputs -->
              <Group id="Group8_Outputs" label="Outputs">
                <Control xsi:type="Button" id="FinalReport" label="Final Report" icon="icon-final-report.png" action="ShowFinalReport" size="Large"/>
                <Control xsi:type="Button" id="ExportPDF" label="Export Report PDF" icon="icon-export-pdf.png" action="ExportPDF" size="Small"/>
                <Control xsi:type="Button" id="ExportWord" label="Export Report Word" icon="icon-export-word.png" action="ExportWord" size="Small"/>
                <Control xsi:type="Button" id="ExportLayoutPDF" label="Export Layout PDF" icon="icon-export-layout-pdf.png" action="ExportLayoutPDF" size="Small"/>
                <Control xsi:type="Button" id="ExportLayoutDXF" label="Export Layout DXF" icon="icon-export-layout-dxf.png" action="ExportLayoutDXF" size="Small"/>
                <Control xsi:type="Button" id="ExportLayoutKMZ" label="Export Layout KMZ" icon="icon-export-layout-kmz.png" action="ExportLayoutKMZ" size="Small"/>
                <Control xsi:type="Button" id="ExportSLDPDF" label="Export SLD PDF" icon="icon-export-sld-pdf.png" action="ExportSLDPDF" size="Small"/>
                <Control xsi:type="Button" id="ExportSLDDXF" label="Export SLD DXF" icon="icon-export-sld-dxf.png" action="ExportSLDDXF" size="Small"/>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://orhanelturk.github.io/Orhan2/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://orhanelturk.github.io/Orhan2/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://orhanelturk.github.io/Orhan2/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://orhanelturk.github.io/Orhan2/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://orhanelturk.github.io/Orhan2/taskpane.html"/>
      </bt:Urls>
    </Resources>
  </VersionOverrides>
</OfficeApp>
